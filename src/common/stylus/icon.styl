@font-face {
  font-family: 'icomoon';
  src: url('../fonts/icomoon.eot?77mohs');
  src: url('../fonts/icomoon.eot?77mohs#iefix') format('embedded-opentype'),
    url('../fonts/icomoon.ttf?77mohs') format('truetype'),
    url('../fonts/icomoon.woff?77mohs') format('woff'),
    url('../fonts/icomoon.svg?77mohs#icomoon') format('svg')
  font-weight: normal
  font-style: normal
}

[class^="elextra-icon-"], [class*=" elextra-icon-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'icomoon' !important
  speak: none
  margin-right: 10px
  font-style: normal
  font-weight: normal
  font-variant: normal
  text-transform: none
  line-height: 1

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased
  -moz-osx-font-smoothing: grayscale
}

